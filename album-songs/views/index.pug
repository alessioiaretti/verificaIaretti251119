doctype html
html
 head
   title #{title}
   link(rel='stylesheet', href='/css/style.css')
   meta(name="viewport" content="width=device-width, initial-scale=1")
 body
   header.header
         h1 #{title}
   article.container
     each elemento in canzoni
       div.elemento
         h2.elemento-name
          | Titolo: <B style="color: red;"> #{elemento.title} </B> 
         a(href=`/song?song_id=${elemento.song_id}`)
          | Visualizza
           

